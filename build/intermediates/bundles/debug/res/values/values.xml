<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="errorcolor">#DE0707</color>
    <color name="gray">#D4D4D4</color>
    <color name="gray_30">#4D000000</color>
    <color name="gray_50">#80E2E2E2</color>
    <color name="gray_card">#D5D5D5</color>
    <color name="gray_color">#E2E2E2</color>
    <color name="linkercolor">#88bf00</color>
    <color name="linkercolor_30">#4D88bf00</color>
    <color name="linkercolor_50">#8088bf00</color>
    <color name="linkercolor_80">#CC88bf00</color>
    <color name="tinkercolor">#9c27b0</color>
    <color name="tinkercolor_30">#4D9c27b0</color>
    <color name="tinkercolor_50">#809c27b0</color>
    <color name="tinkercolor_80">#CC9c27b0</color>
    <color name="warningcolor">#ECD420</color>
    <color name="white">#FFFFFF</color>
    <declare-styleable name="SwipeListView"><attr format="boolean" name="swipeOpenOnLongPress"/><attr format="boolean" name="onlyOneOpenedWhenSwipe"/><attr format="integer" name="swipeAnimationTime"/><attr format="dimension" name="swipeOffsetLeft"/><attr format="dimension" name="swipeOffsetRight"/><attr format="boolean" name="swipeCloseAllItemsWhenMoveList"/><attr format="reference" name="swipeFrontView"/><attr format="reference" name="swipeBackView"/><attr format="enum" name="swipeMode"><enum name="none" value="0"/><enum name="both" value="1"/><enum name="right" value="2"/><enum name="left" value="3"/></attr><attr format="enum" name="swipeActionLeft"><enum name="reveal" value="0"/><enum name="dismiss" value="1"/><enum name="choice" value="2"/></attr><attr format="enum" name="swipeActionRight"><enum name="reveal" value="0"/><enum name="dismiss" value="1"/><enum name="choice" value="2"/></attr><attr format="reference" name="swipeDrawableChecked"/><attr format="reference" name="swipeDrawableUnchecked"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="fab_margin">16dp</dimen>
    <dimen name="loader_padding">2dp</dimen>
    <dimen name="loader_radius">5dp</dimen>
    <dimen name="toolbar_elevation">8dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">Core</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/white</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="ThemeToolbar" parent="Theme.AppCompat.Light">

        <!-- Tool bar background -->
        <item name="android:background">@android:color/transparent</item>
        <!-- navigation icon color -->
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlHighlight">@color/white</item>
        <!-- colorPrimaryDark is used for the status bar -->
        <!--<item name="colorPrimary">@color/tinkercolor</item>-->
        <item name="colorPrimaryDark">@color/tinkercolor</item>
        <!-- Subtitle color -->
        <item name="android:textColorSecondary">@color/black</item>
        <!-- Title color-->
        <item name="android:textColorPrimary">@color/white</item>

        <item name="android:displayOptions">showHome|homeAsUp|showTitle</item>

    </style>
</resources>